
(use gauche.process)
(use gauche.reload)
(use file.util)
(use srfi-1)
(use kirjasto)


(add-load-path "/usr/local/")

(read-eval-print-loop #f #f 
                      (lambda results
                        (map (cut print "=> " <>) results)) 
                      (lambda () 
                            (display (make-colour 33 ">> ")) 
                        (flush)))
