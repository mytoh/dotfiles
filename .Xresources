Xcursor.theme: whiteglass

urxvt.termName: xterm
urxvt.background: #000000
urxvt.foreground: [90]snow
urxvt.geometry: 100x40+100+100
urxvt.depth: 32
urxvt.scrollBar: false
urxvt.buffered: true
!urxvt.borderLess: true
urxvt.internalBorder: 1
urxvt.transparent: true
urxvt.fade: 50%
urxvt.fadeColor: black
urxvt.shading: 30
urxvt.blurradius: 2
!urxvt.tintColor: #ffffff
urxvt.preedittype: root
urxvt.savelines: 2000
urxvt.mousewheelscrollpage: true
urxvt.urgentOnBell: true

urxvt.perl-ext-common: default,matcher
urxvt.urlLauncher: firefox3
urxvt.matcher.button: 1
urxvt.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-].[\\w/-]

urxvt.font: -mplus-fxd-medium-r-normal--10-*
urxvt.boldFont: -mplus-fxd-medium-r-normal--10-*

! BLACK
*color0: #262729
*color8: #554444
! MAGENTA
*color1: #F92672
*color9: #FF669D
! BLUE
*color2: #7441ff
*color10: #391f80
! YELLOW
*color3: #FFD800
*color11: #CCAD00
! GREEN
*color4: #21802f
*color12: #52fc6f
! PERIWINKLE
*color5: #9E6FFE
*color13: #9E6FFE
! LIGHT BLUE
*color6: #66D9EF
*color14: #66D9EF
! WHITE
*color7: #CCCCC6
*color15: #F8F8F2

