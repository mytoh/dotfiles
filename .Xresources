urxvt.fading: 2%
urxvt.fadeColor: grey
urxvt.shading: 20
urxvt.tintcolor: black
urxvt.savelines: 2000
urxvt.mousewheelscrollpage: false
urxvt.urgentOnBell: true
urxvt.modifier: alt
urxvt.inputMethod: uim
! urxvt.preeditType: OnTheSpot
urxvt.preeditType: OverTheSpot
!urxvt.backgroundPixmap: /Users/kazuki/.wallpapers/background

urxvt.perl-ext-common: default,matcher,clipboard-osc,tabbbed,xim-onthespot
