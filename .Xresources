#--------------urxvt---------
urxvt*background: black
urxvt*foreground: snow
urxvt*scrollBar: False
urxvt*transparent: true
urxvt*inheritPixmap: True
urxvt*tintColor: pink
urxvt*shading: 15
urxvt*preeditType: root
urxvt*depth: 32
urxvt*saveLines: 100000

urxvt*font: \
    xft:Inconsolata:size=10:antialias=true,\
    xft:M+ 1p:size=9:style=regular:antialias=true
urxvt*boldFont: xft:Inconsolata:style=Bold:size=10:antialias=true
 

urxvt*color0: #000000
urxvt*color1: #CD0000
urxvt*color2: #00CD00
urxvt*color3: #CDCD00
urxvt*color4: #1E90FF
urxvt*color5: #CD00CD
urxvt*color6: #00CDCD
urxvt*color7: #E5E5E5
urxvt*color8: #4C4C4C
urxvt*color9: #FF0000
urxvt*color10: #00FF00
urxvt*color11: #FFFF00
urxvt*color12: #4682B4
urxvt*color13: #FF00FF
urxvt*color14: #00FFFF
urxvt*color15: #FFFFFF

