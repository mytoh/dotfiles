xft.dpi: 98
xft.antialias: true
xft.hinting: true
xft.hintstyle: hintslight

Xcursor.theme: whiteglass

#include ".xresources.d/urxvt"


!-------------------------------------
! colors
!----------------------------------
!
! relative path from $HOME
#include ".xcolours/raven-dark"
!*color0: black
!*color1: red3
!*color2: green3
!*color3: yellow3
!*color4: blue2
!*color5: magenta3
!*color6: cyan3
!*color7: gray90
!*color8: gray50
!*color9: red
!*color10: green
!*color11: yellow
!*color12: rgb:5c/5c/ff
!*color13: magenta
!*color14: cyan
!*color15: white
!--------------------------------------

!-------------
! xterm
! ------------
xterm*font: -nil-profont-medium-r-normal--10-100-72-72-c-50-iso8859-1
xterm*utf8: always
xterm*utf8Fonts: always
xterm*title: xterm
xterm*faceName: terminus:pixelsize=10
xterm*geometry: 90x25
xterm*boldMode: false
xterm*cutNewline: false
xterm*on4Clicks: group
xterm*on5Clicks: page
xterm*internalBorder: 2
xterm*background: #222222
xterm*foreground: #ddccbb
xterm*customization: color
xterm*metaSendsEscape: true
xterm*colorBD: #afd700
xterm*colorBDMode: false
xterm*cursorBlink: false
xterm*cursorColor: #ff9900
xterm*dynamiccolors: true
xterm*loginShell: true
xterm*charClass: 33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48,35:48,58:48,63:48,61:48,44:48,38:48,59:48
xterm*jumpScroll: true
xterm*multiScroll: true
xterm*saveLines: 512
xterm*scrollBar: false
xterm*xftAntialias: false



!-------------
! xfontsel
! -----------
!XFontSel*sampleText.background: TEXTBG
XFontSel.font: -artwiz-lime-medium-r-normal--10-110-75-75-m-50-iso8859-1
XFontSel*menu*showUnselectable: false
XFontSel*MenuButton.shadowWidth: 0
XFontsel*Toggle*ToggleStyle: check
XFontsel*MenuButton*Justify:    left
XFontsel*MenuButton*MenuButtonStyle: select
.xfontsel.pane.commandBox.quitButton.background: #777777
.xfontsel.pane.commandBox.quitButton.foreground: snow
