!urxvt.termName: xterm-256color
urxvt.depth: 24
urxvt.background: rgba:0940/0990/1100/cccc
urxvt.foreground: #99a6bd
urxvt.geometry: 100x40+100+100
urxvt.scrollBar: false
urxvt.buffered: true
urxvt.borderLess: false
urxvt.internalBorder: 0
urxvt.cursorUnderline: false
urxvt.cursorColor: #4779B3
!urxvt.transparent: false
