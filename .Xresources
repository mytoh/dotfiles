Xcursor.theme: whiteglass

urxvt.termName: xterm
urxvt.background: #000000
urxvt.foreground: [90]snow
urxvt.depth: 32
urxvt.scrollBar: false
urxvt.buffered: true
!urxvt.borderLess: true
urxvt.internalBorder: 1
urxvt.transparent: true
urxvt.fade: 50%
urxvt.fadeColor: black
urxvt.shading: 30
urxvt.blurradius: 2
!urxvt.tintColor: #ffffff
urxvt.preedittype: root
urxvt.savelines: 2000
urxvt.mousewheelscrollpage: true
urxvt.urgentOnBell: true

urxvt.perl-ext-common: default,matcher
urxvt.urlLauncher: firefox3
urxvt.matcher.button: 1
urxvt.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-].[\\w/-]

urxvt.font: -mplus-fxd-medium-r-normal--10-*
urxvt.boldFont: xft:Inconsolata:style=Bold:size=10:antialias=true, \
                xft:M+ 1p:size=9:style=:bold:antialias=true

urxvt.color12: #7fb4ff

!urxvt.color0: #000000
!urxvt.color1: #CD0000
!urxvt.color2: #00CD00
!urxvt.color3: #CDCD00
!urxvt.color4: #1E90FF
!urxvt.color5: #CD00CD
!urxvt.color6: #00CDCD
!urxvt.color7: #E5E5E5
!urxvt.color8: #4C4C4C
!urxvt.color9: #FF0000
!urxvt.color10: #00FF00
!urxvt.color11: #FFFF00
!urxvt.color12: #2b4f98
!urxvt.color13: #FF00FF
!urxvt.color14: #00FFFF
!urxvt.color15: #FFFFFF


