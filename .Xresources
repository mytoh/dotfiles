xft.dpi: 98
xft.hinting: true
xft.hintstyle: hintslight
Xcursor.theme: whiteglass

!---------------
! urxvt
!---------------
urxvt.termName: xterm-256color
urxvt.depth: 24
!urxvt.background: rgba:0900/0900/0950/cccc
!urxvt.foreground: #575757
urxvt.geometry: 100x40+100+100
urxvt.scrollBar: false
urxvt.buffered: true
urxvt.borderLess: false
urxvt.internalBorder: 0
!urxvt.transparent: false
!urxvt.inheritPixmap: true
urxvt.fading: 2%
urxvt.fadeColor: grey
urxvt.shading: 20
urxvt.tintcolor: black
urxvt.preedittype: Root
urxvt.savelines: 2000
urxvt.mousewheelscrollpage: false
urxvt.urgentOnBell: true

urxvt.perl-ext-common: default,matcher,clipboard-osc
urxvt.urlLauncher: firefox
urxvt.matcher.button: 1
urxvt.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-].[\\w/-]

!urxvt.font:Terminus\-ja-12
!urxvt.boldFont:Terminus\-ja-12
!urxvt.font: k12
!urxvt.boldfont: k12
urxvt.font: -*-terminus-medium-r-normal-*-12-*-*-*-*-*-*-*, \
            -mplus-gothic-medium-r-normal-*-10-*-*-*-*-*-*-*

urxvt.boldFont: -*-terminus-medium-r-normal-*-12-*-*-*-*-*-iso8859-*, \
            -*-terminus-medium-r-normal-*-12-*-*-*-*-*-iso10646-*, \
             -*-terminus-medium-r-normal-*-12-*-*-*-*-*-*-*, \
            -mplus-gothic-medium-r-normal-*-10-*-*-*-*-*-*-*

!urxvt.boldfont: -mplus-gothic-medium-r-normal-*-10-*-*-*-*-*-*-*
!urxvt.boldFont: 9x15bold,\
!    -mplus-gothic-medium-r-normal-*-10-*-*-*-*-*-*-*, \
!    -misc-fixed-bold-r-normal--15-140-75-75-75-c-90-iso10646-1,\
!    -misc-fixed-medium-r-normal--15-140-75-75-c-90-iso10646-1,\
!    [codeset=JISX0208]xft:Kochi Gothic:antialias=false, \
!    xft:Code200:antialias=false




