Xcursor.theme: whiteglass

!---------------
! urxvt
!---------------
urxvt.termName: xterm
urxvt.depth: 32
urxvt.background: rgba:0000/0000/0000/cccc
!urxvt.background: rgb:0000/0000/0000
urxvt.foreground: white
urxvt.geometry: 100x40+100+100
urxvt.scrollBar: false
urxvt.buffered: true
!urxvt.borderLess: true
urxvt.internalBorder: 0
!urxvt.transparent: true
urxvt.fading: 2%
urxvt.fadeColor: grey
urxvt.shading: 10
urxvt.tintcolor: black
urxvt.preedittype: root
urxvt.savelines: 2000
urxvt.mousewheelscrollpage: true
urxvt.urgentOnBell: true

urxvt.perl-ext-common: default,matcher
urxvt.urlLauncher: firefox
urxvt.matcher.button: 1
urxvt.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-].[\\w/-]

!urxvt.font: xft: aqua_pfont-9
!urxvt.boldFont: xft: aqua_pfont-9
urxvt.font: xft: Bitstream Vera Sans Mono-9
urxvt.boldFont: xft: Bitstream Vera Sans Mono-9
!urxvt.font: -shinonome-gothic-medium-r-normal--12-*
!urxvt.boldFont: -shinonome-gothic-bold-r-normal-*-12-*
!urxvt.font: k12
!urxvt.boldfont: k12

!-------------
! xterm
! ------------
xterm*foreground: white
xterm*background: black
xterm*scrollBar: false


