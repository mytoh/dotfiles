
set -f path=(${HOME}/local/bin ${path})

setenv shellar ${HOME}/.config/shellar
setenv shellar_custom ${HOME}/.shellar_custom

setenv shellar_theme 'default'

set -f bottle_radio=(radio bbc)
set -f bottle_scheme=(loitsu lehti nopea napa mosh ypsilon talikko pikkukivi)
set -f shellar_bottles=( mytoh emacs ${bottle_scheme} fasd \
                      ${bottle_radio} cask vim nyan \
                       brew npm freebsd firefox please percol \
                      sls)

setenv shellar_bottles "${shellar_bottles}"

set -f path=(${shellar}/bin ${path})

eval "`shellar init`"
