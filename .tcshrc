
set shellar=$HOME/.shellar

if ( "$OSTYPE" == "FreeBSD" ) then
set shellar_plugins=(mytoh emacs loitsu lehti fasd nopea napa \
               vim lista nyan ypsilon mosh talikko brew npm freebsd firefox)
if ( { ( type kehote >& /dev/null ) } ) then
set shellar_theme='kehote'
else
set shellar_theme='default'
endif
else
set shellar_plugins=(mytoh emacs gauche loitsu lehti fasd nopea napa \
               vim lista nyan ypsilon mosh talikko brew npm  panna pikkukivi \
               firefox)
set shellar_theme='default'
endif

source $shellar/shellar/shellar.tcsh

foreach f ($HOME/.complete.tcsh $HOME/perl5/perlbrew/etc/cshrc)
  if (-f $f) then
    source $f
  endif
end

