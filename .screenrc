
escape z
defutf8 on
encoding utf-8 utf-8
defencoding utf-8
defkanji utf-8
vbell on
bell_msg "bell: %n (%t) [%w:%s]"
shelltitle '> |tcsh'
startup_message off
autodetach on
bind q quit
bind k kill
bind s 
defmonitor off
defscrollback 10000
altscreen on

term screen
attrcolor b ".I"
termcapinfo xterm 'Co#256:AB=\E[48;5;%dm:AF=\E[38;5;%dm'
defbce on

caption always "%{= wk} %?%F%{b kw}%? %?%h%:%t (screen #%n) %{-}"
#hardstatus alwayslastline "%`%-w%{= dd}%n %t%{-}%+w [%02c]"
backtick 0 0 0 `whoami`
hardstatus alwayslastline "%{= dd} %-Lw%40L>%{= dB} %n %t %{-}%+Lw%-040=%{= db}  %0@%H%{= dM} %l %{= dB} %Y/%m/%d %C:%s %A "


