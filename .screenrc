
escape z
defutf8 on
encoding utf-8 utf-8
defencoding utf-8
defkanji utf-8
vbell on
bell_msg "bell: %n (%t) [%w:%s]"
shelltitle '> |tcsh'
startup_message off
autodetach on
defmonitor off
defscrollback 10000
altscreen on
defflow off

bind q quit
bind k kill
bind -c REGION 0 remove
bind -c REGION 1 only
bind -c REGION 2 split
#bind -c REGION 3 split -v
bind -c REGION o focus
bind -c REGION \^ resize
bind ^x command -c REGION
bindkey ^t focus

attrcolor b ".I"
termcapinfo xterm 'Co#256:AB=\E[48;5;%dm:AF=\E[38;5;%dm'
defbce on

caption always "%{= wk} %?%F%{b kw}%?%t (screen #%n) %?[%h]%?%{-}"
backtick 0 0 0 whoami
hardstatus alwayslastline "%{= dd} %-Lw%40L>%{= bk} %n %t %{-}%+Lw%-055=%{= gk} %`@%H %{= Mk} %l %{= bk} %Y/%m/%d %C:%s %A %="
sorendition "= dd"



