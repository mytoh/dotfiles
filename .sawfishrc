(require 'sawfish-defaults)
(require 'sawfish.wm.ext.pager)
(setq backtrace-on-error t)

(setq pager-executable "pager")
(add-hook 'after-initialization-hook pager t)

(setq *workspace-count* 4)

;; sawfish user customization -- do not edit by hand!
;; sawfish version 1.5.2, written Wed Dec 15 20:28:20 2010

(custom-set-typed-variable (quote pager-workspaces-per-column) (quote 1) (quote number))
(custom-set-typed-variable (quote pager-show-ignored-windows) (quote t) (quote boolean))
(custom-set-typed-variable (quote resize-show-dimensions) (quote ()) (quote boolean))
(custom-set-typed-variable (quote move-show-position) (quote ()) (quote boolean))
(custom-set-typed-variable (quote ignore-program-positions) (quote ()) (quote boolean))
(custom-set-typed-variable (quote place-window-mode) (quote top-left) (quote symbol))
(custom-set-typed-variable (quote window-history-auto-save-dimensions) (quote ()) (quote boolean) (quote sawfish.wm.ext.window-history))
(custom-set-typed-variable (quote Dust:icon-mode) (quote if-available) (quote (choice (always "Always (use default icon where necessary)") (if-available "If available (don't use the default icon)") (never "Never"))))
(custom-set-typed-variable (quote Dust:show-resize-triangle) (quote t) (quote boolean))
(custom-set-typed-variable (quote tooltips-enabled) (quote ()) (quote boolean) (quote sawfish.wm.ext.tooltips))
(custom-set-typed-variable (quote default-window-animator) (quote none) (quote (choice none solid wireframe cross elliptical draft)))
(custom-set-typed-variable (quote resize-outline-mode) (quote opaque) (quote (choice opaque box cross elliptical draft)))
(custom-set-typed-variable (quote move-outline-mode) (quote opaque) (quote (choice opaque box cross elliptical draft)))
(custom-set-typed-variable (quote default-frame-style) (quote Dust) (quote frame-style))
(custom-set-typed-variable (quote default-font) (quote ("Pango" . "Inconsolata Medium 10")) (quote font))
(custom-set-typed-variable (quote Crux:button-theme) (quote next) (quote (choice (default "Default") (platinum "Mac OS Platinum") (macos-x "Mac OS X") (windows "MS Windows") (next "NeXTSTEP") (complete "Complete") (complete-inverse "Complete (Inverse)"))))
(custom-set-typed-variable (quote Crux:show-window-icons) (quote t) (quote boolean))
