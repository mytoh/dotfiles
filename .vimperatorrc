noremap h gT
noremap l gt
noremap : ;
noremap ; :

cnoremap <c-i> <tab>
colorscheme sweets

set hintchars=aoeuidhtns
set complete=lsb
"- ddg with google suggestions 
"  http://ddgg.nfriendly.com/#oneclick
"- edit about:config/keyword.url for parameterize url
set defsearch="google"
set editor="vim"
set gui=notabs
set toolbars=noaddons,nobookmarks,menu,nonavigation,tabs

" plugin_loader
let g:plugin_loader_roots = "~/.vimperator/vimperator-plugins/"
let g:plugin_loader_plugins = "_libly,delicious_search,stella,autoIgnoreKey,direct_bookmark,prevent-pseudo-domain,statusline-toolbar,unload-tab,walk-input"

"direct_bookmark.js
let g:direct_sbm_use_services_by_tag = "hd"
let g:direct_sbm_use_services_by_post = "d"

command! -nargs=+ lazy autocmd VimperatorEnter .* <args>
" Google Reader
lazy fmaps -u='www\.google\.(co\.jp|com)/reader' -events=vkeypress j k n p m s v A r S N P X O gh gd gs gt gu u / ? J K
" Google Reader Full Feed
lazy fmaps -u='www\.google\.(co\.jp|com)/reader' -events=vkeyup z

" statusline-toolbar
"set statustoolbars=bookmarks-menu-button-container

" delicious_search.js
autocmd VimperatorEnter ".*" :set complete+=D

